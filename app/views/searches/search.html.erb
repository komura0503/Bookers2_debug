<div class='container px-5 px-sm-0'>
  <div class='row'>
    <div class='col-md-12'>
    <% if @model == "User" %>
      <h2>Users search for"<%= @content %>"</h2>
      <%= render 'users', users: @users %>
    <% else %>
      <h2>Books search for"<%= @content %>"</h2>
      <%= render 'index', books: @books %>
    <% end %>
    </div>
  </div>
</div>